# Makefile for example IDA data clients and utilities

CC     = cc
VPATH  =
INCDIR = include
BINDIR = .
OPTMIZ = -g
INCS   = -I$(INCDIR)
DEFS   = 
CFLAGS = $(OPTMIZ) $(INCS) -L$(LIBDIR) $(DEFS)
LIB    = libidatap.a
LIBDIR = .
OUTPUT = eztap 

# uncomment the following if required (eg, Solaris)
SOCKLIBS = -lsocket -lnsl

eztap: eztap.c 
	$(CC) $(CFLAGS) -o $@ eztap.c  -lidatap $(SOCKLIBS)
