BIN=dbmap_gui		# commands to be placed in bin
PF= dbmap_gui.pf		# parameter files to be placed in data/pf

MAN1=dbmap_gui.1 	# man pages for commands
CLEAN= .make.state		# Extra files which should be removed during a "make clean"

