
ORBDLIB= _orb.so 
DLIB=python__orb$(DSUFFIX)

DATADIR= python/antelope

DATA= $(ORBDLIB) orb.py 

MAN1=
MAN3= 
MAN5=

CLEAN= *.pyc *.so

cflags= -I$(PYTHONINCLUDE)
fflags=
ldflags= 
ldlibs= $(ORBLIBS)

include $(ANTELOPEMAKE)
include $(ANTELOPEMAKE).local

ORBOBJS= _orb.o

$(ORBDLIB) : $(ORBOBJS)
	$(RM) $@
	$(DLD) $(DLDFLAGS) -o $(ORBDLIB) $(ORBOBJS) $(DLDLIBS) 

$(DLIB): $(ORBDLIB)
	$(RM) $@
	@cp $(ORBDLIB) $(DLIB)
